<script>

    // imagenes baner 1
    let nube1 = document.getElementById('nube1');
    let nube2 = document.getElementById('nube2');
    let monta1 = document.getElementById('monta1');
    let monta2 = document.getElementById('monta2');
    let monta3 = document.getElementById('monta3');
    let montaagua = document.getElementById('montaagua');
    let montaarse = document.getElementById('montaarse');
    let montallama = document.getElementById('montallama');
    let arse = document.getElementById('arse');
    let titulo = document.getElementById('titulo');


    // imagenes baner 2



    window.addEventListener('scroll', function () {
        let value = window.scrollY;
        var ancho = screen.width;
        if (ancho >= 1400) {
            if (value <= 350) {

                montaarse.style.top = value * -0.20 + 'px';
                arse.style.top = value * -0.28 + 'px';
                montallama.style.top = value * -0.28 + 'px';

                nube1.style.top = value * 0.80 + 'px';
                nube2.style.top = value * -0.28 + 'px';


                monta2.style.top = value * 0.25 + 'px';
                monta3.style.top = value * -0.10 + 'px';
                montaagua.style.top = value * 0.25 + 'px';
                titulo.style.top = value * 1.4 + 'px';
            }
            else {
                return null
            }
        } else {
            return null;
        }


    })

</script>


<script>
    window.addEventListener('scroll', (e) => {
        let y = window.scrollY;

        if (y >= 500) {
            document.getElementById('nav').classList.add('my-nav')
        } else {
            document.getElementById('nav').classList.remove('my-nav')
        }
    })

    function navBarColor() {
        document.getElementById('nav').classList.add('my-nav')
    }


</script>

