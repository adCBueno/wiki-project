<script src="https://2021.igem.org/wiki/index.php?title=Template:Bolivia/ButtonFlo&action=raw&ctype=text/javascript"></script>
<script>
function navBarColor() {
        document.getElementById('nav').classList.add('my-nav')
    }
    window.addEventListener('scroll', (e) => {
        let y = window.scrollY;
        if (y >= 500) {
            document.getElementById('nav').classList.add('my-nav')
        } else {
            document.getElementById('nav').classList.remove('my-nav')

        }
        if (y > 9800) {
            document.getElementById('pestania2').classList.remove('box-icon-select')
            document.getElementById('pestania3').classList.add('box-icon-select')
            document.getElementById('pestania1').classList.add('box-icon-select')
        } else {
            document.getElementById('pestania3').classList.remove('box-icon-select')
            document.getElementById('pestania2').classList.add('box-icon-select')
            document.getElementById('pestania1').classList.add('box-icon-select')
        }


        if (y > 16472) {
            document.getElementById('pestania1').classList.remove('box-icon-select')
            document.getElementById('pestania2').classList.remove('box-icon-select')
            document.getElementById('pestania3').classList.remove('box-icon-select')
            document.getElementById('pestania4').classList.add('box-icon-select')
            document.getElementById('pestania5').classList.add('box-icon-select')
        } else {
            document.getElementById('pestania4').classList.remove('box-icon-select')
            document.getElementById('pestania5').classList.remove('box-icon-select')
        }
        
        if(y>17448){
            document.getElementById('pestania4').classList.add('box-icon-select')
            document.getElementById('pestania6').classList.add('box-icon-select')
            document.getElementById('pestania5').classList.remove('box-icon-select')
        }else{
            document.getElementById('pestania6').classList.remove('box-icon-select')
        }

        if(y>18448){
            document.getElementById('pestania4').classList.add('box-icon-select')
            document.getElementById('pestania7').classList.add('box-icon-select')
            document.getElementById('pestania6').classList.remove('box-icon-select')
        }else{
            document.getElementById('pestania7').classList.remove('box-icon-select')
        }
        if(y>19448){
            document.getElementById('pestania4').classList.add('box-icon-select')
            document.getElementById('pestania8').classList.add('box-icon-select')
            document.getElementById('pestania7').classList.remove('box-icon-select')
        }else{
            document.getElementById('pestania8').classList.remove('box-icon-select')
        }
        if(y>20448){
            document.getElementById('pestania4').classList.add('box-icon-select')
            document.getElementById('pestania9').classList.add('box-icon-select')
            document.getElementById('pestania8').classList.remove('box-icon-select')
        }else{
            document.getElementById('pestania9').classList.remove('box-icon-select')
        }
        

    })


</script>

